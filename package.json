{
  "name": "combobulate",
  "version": "1.0.5",
  "description": "Flexible neural net implementation for unstructured data.",
  "main": "./lib/index.bundle.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib/**/**.d.ts",
    "lib/index.bundle.js"
  ],
  "scripts": {
    "precommit": "npm run test && pretty-quick",
    "prepublishOnly": "tsc && npm run build",
    "build": "rollup -c",
    "test": "jest"
  },
  "keywords": [
    "Neural Net",
    "Deep Learning",
    "Convolution",
    "Normalization",
    "Lightweight"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DuncanWalter/combobulate.git"
  },
  "author": "Duncan Walter",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DuncanWalter/combobulate/issues"
  },
  "homepage": "https://github.com/DuncanWalter/combobulate#readme",
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(\\.|/)test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "avoid",
    "tabWidth": 2
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "husky": "^0.14.3",
    "jest": "^23.5.0",
    "prettier": "^1.14.2",
    "pretty-quick": "^1.6.0",
    "rollup": "^0.66.6",
    "rollup-plugin-typescript": "^1.0.0",
    "ts-jest": "^23.1.4",
    "tslib": "^1.9.3",
    "typescript": "^3.0.3",
    "webpack": "^4.21.0"
  },
  "dependencies": {}
}

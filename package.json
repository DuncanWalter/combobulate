{
  "name": "combobulate",
  "version": "1.0.0",
  "description": "Flexible neural net implementation for unstructured data.",
  "main": "src/index.js",
  "scripts": {
    "precommit": "npm run test && pretty-quick",
    "build": "tsc",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DuncanWalter/combobulate.git"
  },
  "author": "Duncan Walter",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DuncanWalter/combobulate/issues"
  },
  "homepage": "https://github.com/DuncanWalter/combobulate#readme",
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(\\.|/)test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "avoid",
    "tabWidth": 2
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "husky": "^0.14.3",
    "jest": "^23.5.0",
    "prettier": "^1.14.2",
    "pretty-quick": "^1.6.0",
    "ts-jest": "^23.1.4",
    "typescript": "^3.0.3"
  }
}
